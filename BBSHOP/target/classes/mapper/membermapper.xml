<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace ="com.bbshop.bit.member.mapper">


<select id="memberCheck" resultType="int">

	select count(id)
	from sh_user
	where id=#{member_id} and pw =#{member_pw}


</select>








